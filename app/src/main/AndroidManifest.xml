<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sarau_ete_2019">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logosarau"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logosarau"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Web"
            android:label="@string/title_activity_web"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SobreActivity"
            android:label="@string/title_activity_sobre"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventosActivity"
            android:label="@string/title_activity_eventos"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PlayerActivity"
            android:label="@string/title_activity_player"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomenageadoActivity"
            android:label="@string/title_activity_homenageado"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MapaActivity"
            android:label="@string/title_activity_mapa"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LocalizacaoActivity"
            android:label="@string/title_activity_localizacao"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="nosensor"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>